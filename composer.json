{
    "name": "jtrw/php-make",
    "description": "Simple collections of make commands to increase productivity",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Nil Borodulia",
            "email": "nil.borodulia@gmail.com"
        }
    ],
    "require": {
        "composer-plugin-api": "*"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Jtrw\\Make\\MakefilePlugin::installMakefile"
        ]
    },
    "autoload": {
        "psr-4": {
            "Jtrw\\Make\\": "src/"
        }
    },
    "extra": {
        "class": "Jtrw\\Make\\MakefilePlugin"
    }
}
